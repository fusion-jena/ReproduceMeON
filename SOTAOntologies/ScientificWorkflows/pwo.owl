    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/date"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/rights"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.semanticweb.org/owlapi#nodeID -->

    <owl:AnnotationProperty rdf:about="http://www.semanticweb.org/owlapi#nodeID"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#comment -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#comment"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#label -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#label"/>
    


    <!-- http://www.w3.org/2002/07/owl#versionInfo -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#versionInfo"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/spar/pwo/executes -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/executes">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/basicplan.owl#satisfies"/>
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/isExecutedBy"/>
        <rdfs:comment xml:lang="en">This property allows to link a workflow execution to the related workflow description.</rdfs:comment>
        <rdfs:label xml:lang="en">executes</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/happened -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/happened">
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/isIntervalFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#TimeInterval"/>
        <rdfs:comment xml:lang="en">It allows one to link an event to the interval when it happened.</rdfs:comment>
        <rdfs:label xml:lang="en">happened</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/hasFirstStep -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/hasFirstStep">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/spar/pwo/hasStep"/>
        <rdfs:range>
            <owl:Class>
                <owl:complementOf>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.org/spar/pwo/hasPreviousStep"/>
                        <owl:someValuesFrom rdf:resource="http://purl.org/spar/pwo/Step"/>
                    </owl:Restriction>
                </owl:complementOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">A workflow that starts with a particular step.</rdfs:comment>
        <rdfs:label xml:lang="en">has first step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/hasNextStep -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/hasNextStep">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes"/>
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/hasPreviousStep"/>
        <rdfs:domain rdf:resource="http://purl.org/spar/pwo/Step"/>
        <rdfs:range rdf:resource="http://purl.org/spar/pwo/Step"/>
        <rdfs:comment xml:lang="en">A property linking a step in a workflow with the step that directly follows it.</rdfs:comment>
        <rdfs:label xml:lang="en">has next step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/hasPreviousStep -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/hasPreviousStep">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows"/>
        <rdfs:comment xml:lang="en">A property linking a step in a workflow with the step that directly precedes it.</rdfs:comment>
        <rdfs:label xml:lang="en">has previous step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/hasStep -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/hasStep">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/basicplandescription.owl#definesTask"/>
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/isStepOf"/>
        <rdfs:domain rdf:resource="http://purl.org/spar/pwo/Workflow"/>
        <rdfs:range rdf:resource="http://purl.org/spar/pwo/Step"/>
        <rdfs:comment xml:lang="en">The property linking a workflow to a component step.</rdfs:comment>
        <rdfs:label xml:lang="en">has step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/involvesAction -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/involvesAction">
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/isActionInvolvedIn"/>
        <rdfs:domain rdf:resource="http://purl.org/spar/pwo/WorkflowExecution"/>
        <rdfs:range rdf:resource="http://purl.org/spar/pwo/Action"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://purl.org/spar/pwo/executes"/>
            <rdf:Description rdf:about="http://purl.org/spar/pwo/hasStep"/>
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#isExecutedIn"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">The property linking a particular execution of a workflow to an action.</rdfs:comment>
        <rdfs:label xml:lang="en">involves action</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/isActionInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/isActionInvolvedIn">
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask"/>
            <rdf:Description rdf:about="http://purl.org/spar/pwo/isStepOf"/>
            <rdf:Description rdf:about="http://purl.org/spar/pwo/isExecutedBy"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">The property linking an event to a particular execution of a workflow description.</rdfs:comment>
        <rdfs:label xml:lang="en">is action involved in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/isExecutedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/isExecutedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/basicplan.owl#isSatisfiedBy"/>
        <rdfs:comment>This property allows to link a workflow description to the related workflow execution.</rdfs:comment>
        <rdfs:label>is executed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/isIntervalFor -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/isIntervalFor">
        <rdfs:comment xml:lang="en">It allows one to link an interval describing when an event happened to the related event.</rdfs:comment>
        <rdfs:label xml:lang="en">is interval for</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/isNeededBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/isNeededBy">
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/needs"/>
        <rdfs:comment xml:lang="en">The property linking a thing required to undertake a workflow step or an event (e.g. an action executing a workflow step) to the step/event itself.</rdfs:comment>
        <rdfs:label xml:lang="en">is needed by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/isProducedBy -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/isProducedBy">
        <owl:inverseOf rdf:resource="http://purl.org/spar/pwo/produces"/>
        <rdfs:comment xml:lang="en">The property linking something to the workflow step or an event (e.g. an action executing a workflow step) that produces, creates or results in that thing.</rdfs:comment>
        <rdfs:label xml:lang="en">is produced by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/isStepOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/isStepOf">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/basicplandescription.owl#isTaskDefinedIn"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The property linking a step in a workflow to the workflow of which it is a component.</rdfs:comment>
        <rdfs:label xml:lang="en">is step of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/needs -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/needs">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/spar/pwo/Step"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">The property linking a workflow step or an event (e.g. an action executing a workflow step) to anything required to undertake that step/event.</rdfs:comment>
        <rdfs:label xml:lang="en">needs</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/spar/pwo/produces -->

    <owl:ObjectProperty rdf:about="http://purl.org/spar/pwo/produces">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/spar/pwo/Step"/>
                    <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#Event"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">The property linking a workflow step or an event (e.g. an action executing a workflow step) to the thing that the step/event produces, creates or results in.</rdfs:comment>
        <rdfs:label xml:lang="en">produces</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/basicplandescription.owl#definesTask -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/basicplandescription.owl#definesTask"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/basicplandescription.owl#isTaskDefinedIn -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/basicplandescription.owl#isTaskDefinedIn"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#hasParticipant"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/participation.owl#isParticipantIn"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/situation.owl#isSettingFor"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#executesTask"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#isExecutedIn -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/taskexecution.owl#isExecutedIn"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#atTime">
        <rdfs:comment xml:lang="en">A step is performed within a time interval.</rdfs:comment>
        <rdfs:label xml:lang="en">at time</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#forEntity"/>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#hasTimeIndexedSetting"/>
    


