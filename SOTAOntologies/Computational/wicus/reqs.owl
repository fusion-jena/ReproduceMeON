    <owl:ObjectProperty rdf:about="#requiresExecutionEnvironment">
        <rdfs:comment rdf:datatype="&rdfs;Literal"
            >wreq:requiresExecutionEnvironment is a specific type of dcterms:requires that links a wreq:Worflow to the wreq:ExecutionEnvironment that must be available for executing it.</rdfs:comment>
        <rdfs:range rdf:resource="#ExecutionEnvironment"/>
        <rdfs:domain rdf:resource="#Workflow"/>
    </owl:ObjectProperty>
    


