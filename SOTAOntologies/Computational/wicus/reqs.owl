            >wreq:hasLevel links a wreq:ExecutionEnvironment to its corresponding wreq:RequirementLevel.</rdfs:comment>
        <rdfs:domain rdf:resource="#ExecutionEnvironment"/>
        <rdfs:range rdf:resource="#RequirementLevel"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/wicus-reqs#hasSubworkflow -->

    <owl:ObjectProperty rdf:about="#hasSubworkflow">
        <rdfs:comment rdf:datatype="&rdfs;Literal"
            >wreq:hasSubworkflow links a wreq:Workflow to the subworkflows that compose it.</rdfs:comment>
        <rdfs:domain rdf:resource="#Workflow"/>
        <rdfs:range rdf:resource="#Workflow"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/net/wicus-reqs#isStepOfWorkflow -->

    <owl:ObjectProperty rdf:about="#isStepOfWorkflow">
        <rdfs:comment rdf:datatype="&rdfs;Literal"
            >wreq:isStepOfWorkflow is a specific type of p-plan:isStepOfPlan that relates a wreq:WorkflowStep to the wreqs:Workflow that it belongs to.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&p-plan;isStepOfPlan"/>
        <rdfs:range rdf:resource="#Workflow"/>
        <rdfs:domain rdf:resource="#WorkflowStep"/>
    </owl:ObjectProperty>
    


